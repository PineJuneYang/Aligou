            android:text="钻石会员"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="@dimen/alg_common_11"
            android:textSize="14sp"
            android:textColor="#000000"
            />

        <ImageView
            android:id="@+id/iv_diamond"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/alg_common_into_arrow"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="@dimen/alg_common_15"
            />


    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/alg_common_divider_color_eb"
